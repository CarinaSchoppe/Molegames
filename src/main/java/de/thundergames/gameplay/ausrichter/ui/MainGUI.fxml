<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="400.0" prefWidth="432.0" xmlns="http://javafx.com/javafx/17"
            fx:controller="de.thundergames.gameplay.ausrichter.ui.MainGUI">
   <children>
      <ImageView fitHeight="415.0" fitWidth="439.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../../../../../resources/gamemaster/test.png"/>
         </image>
      </ImageView>
      <Button fx:id="createGame" layoutX="300.0" layoutY="102.0" mnemonicParsing="false" onAction="#onCreateGameAction"
              prefHeight="29.0" prefWidth="122.0" style="-fx-background-color: transparent;"/>
      <Button fx:id="editGame" layoutX="300.0" layoutY="134.0" mnemonicParsing="false" onAction="#onEditGame"
              prefHeight="31.0" prefWidth="121.0" style="-fx-background-color: transparent;"/>
      <Button fx:id="startGame" layoutX="299.0" layoutY="170.0" mnemonicParsing="false" onAction="#onStartGame"
              prefHeight="30.0" prefWidth="121.0" style="-fx-background-color: transparent;"/>
      <Button fx:id="editTournament" layoutX="304.0" layoutY="300.0" mnemonicParsing="false"
              onAction="#onEditTournament" prefHeight="31.0" prefWidth="122.0"
              style="-fx-background-color: Transparent;"/>
      <Button fx:id="createTournament" layoutX="306.0" layoutY="269.0" mnemonicParsing="false"
              onAction="#onCreateTournament" prefHeight="26.0" prefWidth="122.0"
              style="-fx-background-color: transparent;"/>
      <Button fx:id="breakGame" layoutX="13.0" layoutY="371.0" mnemonicParsing="false" onAction="#onBreakGame"
              prefHeight="31.0" prefWidth="122.0" style="-fx-background-color: transparent;"/>
      <Button fx:id="continueGame" layoutX="158.0" layoutY="369.0" mnemonicParsing="false" onAction="#onContinueGame"
              prefHeight="31.0" prefWidth="122.0" style="-fx-background-color: tranparent;"/>
      <Button fx:id="endGame" layoutX="307.0" layoutY="372.0" mnemonicParsing="false" onAction="#onEndGame"
              prefHeight="26.0" prefWidth="122.0" style="-fx-background-color: transparent;"/>
   </children>
</AnchorPane>
