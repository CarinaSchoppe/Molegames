<?import javafx.scene.control.*?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>
<?import java.net.URL?>
<VBox xmlns:fx="http://javafx.com/fxml/1" maxHeight="856.0" maxWidth="900.0" minHeight="856.0" minWidth="900.0"
      prefHeight="856.0" prefWidth="900.0" xmlns="http://javafx.com/javafx/11.0.2"
      fx:controller="de.thundergames.gameplay.player.ui.GameSelection.GameSelection">
    <children>
        <AnchorPane>
            <children>
                <ImageView>
                    <image>
                        <Image url="@../../../../../../../resources/player/pc/GameSelection.png"/>
                    </image>
                </ImageView>
                <Text fx:id="PlayerName" fill="WHITE" layoutX="8.0" layoutY="34.0" strokeType="OUTSIDE"
                      strokeWidth="0.0" text="Spieler:" wrappingWidth="183.9830722808838">
                    <font>
                        <Font name="Arial Black" size="24.0"/>
                    </font>
                </Text>
                <TableView fx:id="gameTable" layoutX="157.0" layoutY="331.0" prefHeight="396.0" prefWidth="608.0">
                    <columns>
                        <TableColumn fx:id="game_Id" prefWidth="147" reorderable="false" resizable="false"
                                     sortable="false" text="ID"/>
                        <TableColumn fx:id="game_Player_Count" prefWidth="147" reorderable="false" resizable="false"
                                     sortable="false" text="Spieler"/>
                        <TableColumn fx:id="game_Type" prefWidth="147" reorderable="false" resizable="false"
                                     sortable="false" text="Typ"/>
                        <TableColumn fx:id="game_State" prefWidth="147" reorderable="false" resizable="false"
                                     sortable="false" text="Status"/>
                    </columns>
                    <stylesheets>
                        <URL value="@../../../style/TableViewStyle.css"/>
                    </stylesheets>
                </TableView>
                <Button layoutX="773.0" layoutY="273.0" mnemonicParsing="false" onAction="#onUpdateClick" opacity="0.0"
                        prefHeight="53.0" prefWidth="52.0" text="Re"/>
                <Button layoutX="123.0" layoutY="805.0" mnemonicParsing="false" onAction="#onWatchGameClick"
                        opacity="0.0" prefHeight="59.0" prefWidth="256.0" text="Spiel beobachten"/>
                <Button layoutX="434.0" layoutY="805.0" mnemonicParsing="false" onAction="#onSignOutClick" opacity="0.0"
                        prefHeight="59.0" prefWidth="206.0" text="Abmelden"/>
            </children>
        </AnchorPane>
    </children>
</VBox>
